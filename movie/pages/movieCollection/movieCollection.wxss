.list-header {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  color: #999999;
  margin-top: 30rpx;
  margin-left: 30rpx;
  /* background-color: #999; */
}
.results {
  padding: 20rpx;
  /* border-bottom:1px solid #999; */
}

.item {
  padding: 20rpx 0;
  border-bottom:1px solid #999 ;
  display: flex;
  justify-content: flex-start;
}
.item > image{
  height: 200rpx;
  width: 150rpx;
  margin-right: 20rpx;
}
.info {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.info > text {
  font-size: 26rpx;
  color: #666;
}
.info .cur {
  font-size: 30rpx;
  color: #333;
}

.noCont {
  height: 40rpx;
  margin-top: 20rpx;
  text-align: center;
  color: #999;
  font-size: 30rpx;
}
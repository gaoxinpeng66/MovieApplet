.list-header {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 35rpx;
  color: #999999;
  margin-top: 30rpx;
  margin-left: 30rpx;
  /* background-color: #999; */
}

.out {
  padding: 30rpx;
}

.out .box {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0;
  border-bottom: 1rpx dashed #e4e4e4;
}

.out .box .pic {
  width: 220rpx;
  height: 130rpx;
}

.out .box .pic .img {
  width: 100%;
  height: 100%;
}

.out .box .text {
  width: 450rpx;
  height: 130rpx;
}

.out .box .title {
  margin-top: 30rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.loading {
  text-align: center;
  padding: 30rpx 0;
  color: #999
}